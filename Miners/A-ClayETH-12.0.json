{
    "Path": "EthDcrMiner64.exe",
    "Uri": "https://github.com/yuzi-co/miners/releases/download/2018.12/ClaymoreDual12.0.7z",
    "SHA256": "6CBF0C6F587ABB8A865EB45C3AEE0274B2B6F354F895DFA941978E585CA5F216",
    "Type": "AMD",
    "Arguments": [
        "-epool #Protocol#://#Server#:#Port#",
        "-ewal #Login#",
        "-epsw #Password#",
        "-esm #EthStMode#",
        "-wd 1",
        "-r -1",
        "-logfile #GroupName#_log.txt",
        "-logsmaxsize 10",
        "-platform 1",
        "-di #DevicesClayMode#",
        "-mport -#APIPort#",
        "-allpools 1",
        "#AlgorithmParameters#"
    ],
    "API": "Claymore",
    "APIPort": 4028,
    "Fee": "$(if ($DeviceGroup.MemoryGB -gt 3){0.01}else{0})",
    "SSL": true,
    "Algorithms": {
        "Ethash_Blake2s|60": "-dcoin blake2s -dcri 60 -dpool #ProtocolDual#://#ServerDual#:#PortDual# -dwal #LoginDual# -dpsw #PasswordDual#",
        "Ethash_Blake2s|65": "-dcoin blake2s -dcri 65 -dpool #ProtocolDual#://#ServerDual#:#PortDual# -dwal #LoginDual# -dpsw #PasswordDual#",
        "Ethash_Blake2s|70": "-dcoin blake2s -dcri 70 -dpool #ProtocolDual#://#ServerDual#:#PortDual# -dwal #LoginDual# -dpsw #PasswordDual#",
        "Ethash_Keccak|08": "-dcoin keccak -dcri 8 -dpool #ProtocolDual#://#ServerDual#:#PortDual# -dwal #LoginDual# -dpsw #PasswordDual#",
        "Ethash_Keccak|10": "-dcoin keccak -dcri 10 -dpool #ProtocolDual#://#ServerDual#:#PortDual# -dwal #LoginDual# -dpsw #PasswordDual#",
        "Ethash_Keccak|12": "-dcoin keccak -dcri 12 -dpool #ProtocolDual#://#ServerDual#:#PortDual# -dwal #LoginDual# -dpsw #PasswordDual#",
        "Ethash|a1": "-asm 1 -mode 1",
        "Ethash|a2": "-asm 2 -mode 1"
    }
}